<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.bottomViewActivites.AccountActivity">


    <com.google.android.material.button.MaterialButton
        android:id="@+id/logOut"
        android:text="Sign Out"
        android:textAllCaps="false"
        android:drawableRight="@drawable/loout"
        android:drawableTint="@color/white"
        android:backgroundTint="@color/main"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_40sdp"
        android:layout_centerInParent="true"/>
    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
        android:id="@+id/bottomNav_account"
        app:mbn_backgroundBottomColor="@color/main_gray"
        app:mbn_countBackgroundColor="#ff6f00"
        app:mbn_circleColor="@color/main"
        app:mbn_countTextColor="#ffffff"
        app:mbn_defaultIconColor="#FFFFFF"
        app:mbn_rippleColor="#2f424242"
        app:mbn_selectedIconColor="@color/white"
        app:mbn_shadowColor="#1f212121"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</RelativeLayout>